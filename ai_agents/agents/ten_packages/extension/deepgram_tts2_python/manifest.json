{
  "type": "extension",
  "name": "deepgram_tts2_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.10"
    },
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "0.6"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "BUILD.gn",
      "**.tent",
      "**.py",
      "README.md",
      "tests/**"
    ]
  },
  "api": {
    "interface": [
      {
        "import_uri": "../../system/ten_ai_base/api/tts-interface.json"
      }
    ],
    "property": {
      "properties": {
        "api_key": {
          "type": "string"
        },
        "model": {
          "type": "string"
        },
        "voice": {
          "type": "string"
        },
        "encoding": {
          "type": "string"
        },
        "sample_rate": {
          "type": "int64"
        },
        "container": {
          "type": "string"
        },
        "use_rest_fallback": {
          "type": "bool"
        },
        "websocket_timeout": {
          "type": "float64"
        },
        "min_audio_threshold": {
          "type": "int64"
        },
        "reconnect_attempts": {
          "type": "int64"
        },
        "reconnect_delay": {
          "type": "float64"
        },
        "keepalive_interval": {
          "type": "float64"
        },
        "max_request_retries": {
          "type": "int64"
        },
        "health_check_timeout": {
          "type": "float64"
        },
        "dump": {
          "type": "bool"
        },
        "dump_path": {
          "type": "string"
        }
      }
    },
    "cmd_in": [
      {
        "name": "flush"
      }
    ],
    "cmd_out": [
      {
        "name": "flush"
      }
    ]
  }
}
